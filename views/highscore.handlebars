    <div class="container"> 
            <!-- Title -->
            <div class="row mt-5 mb-5">
                <div class="col-md-12">  
                    <h1>
                        <div class="d-flex justify-content-center">
                            <span id="E">E</span>
                            <span id="X">X</span> 
                            <span id="O">O</span>
                            <span id="D">D</span>
                            <span id="U">U</span>
                            <span id="S">S</span>
                        </div>
                    </h1>
                </div>  
            </div>
            <!--High Score Display-->
            <div class="row table-title pb-2 mb-1">
                <div class="col name" style="text-align: center;">
                <h4>Name</h4>
                </div>
                <div class="col deathchart">
                <h4>Number of Deaths</h4>
                </div>
            </div>
            <div class="row mb-5 justify-content-center">
                <div class="col-md-9 options tableFixHead">
                    <table class="table" >
                        
                        <tbody >
                            {{#each highscore_data}}
                          <tr>
                            <th scope="row"></th>
                            <td scope="col">{{username}}</td>
                            <td scope="col">{{score}}</td>
                          </tr>
                          {{/each}}
                        </tbody>
                      </table>
                </div>
            </div>
            <!-- High Score Form -->
            <div class="row mt-5 mb-5 d-flex justify-content-center">
                <div class="col-md-6 submitform text-center">
                    <form>
                        <input type="text" class="form control" name="high-score" id="log-score">
                        <button type="submit" class="btn btn-dark button-styling" id="submit-score">Sign Death Certificate</button>
                    </form>
                </div>
            </div>
            <!--Return to RPG Button-->
            <div class="row mt-5 mb-5 d-flex justify-content-center">
                <div class="col-md-6 submitform text-center">            
                        <button onclick="window.location.href='/rpg.html'" type="submit" class="btn btn-dark btn-lg button-styling" id="submit-score">Return To The Nightmare</button>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 d-flex justify-content-center">
                   <img src="assets/favicon/inverse.png" class="img-fluid logo" id="house"> 
                </div>
            </div>
        </div>  